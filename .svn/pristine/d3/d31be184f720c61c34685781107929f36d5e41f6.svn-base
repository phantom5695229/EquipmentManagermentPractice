package beans;

import java.io.Serializable;
import java.time.LocalDateTime;

public class BuyRecord implements Serializable{
	private static final long serialVersionUID = -2054279353530928006L;
	private User user;
	private Equipment equipment;
	private int count;
	private LocalDateTime time;
	public BuyRecord() {
		super();
	}
	public BuyRecord(User user, Equipment equipment, int count, LocalDateTime time) {
		super();
		this.user = user;
		this.equipment = equipment;
		this.count = count;
		this.time = time;
	}
	@Override
	public String toString() {
		return "buyRecord [user=" + user + ", equipment=" + equipment + ", count=" + count + ", time=" + time + "]";
	}
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	public Equipment getEquipment() {
		return equipment;
	}
	public void setEquipment(Equipment equipment) {
		this.equipment = equipment;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public LocalDateTime getTime() {
		return time;
	}
	public void setTime(LocalDateTime time) {
		this.time = time;
	}
	
	

}
